<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    @*<meta name="viewport" content="width=device-width, initial-scale=1.0">*@
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <title>@ViewBag.Title - ShopOnline</title>
    @Styles.Render("~/Content/fonts/awesome/css") 
    @Styles.Render("~/Content/fonts/ionic/css") 
    @Styles.Render("~/Content/plugins/css") 
        @Styles.Render("~/Content/css")
   
    <link href="~/Theme/plugins/iCheck/minimal/blue.css" rel="stylesheet" />
  
    @Scripts.Render("~/bundles/jquery")
</head>
<body class="skin-blue sidebar-mini">
    <div class="wrapper">
        <header class="main-header">
            @{Html.RenderAction("Header", "Admin");}
        </header>
        <aside class="main-sidebar">
            @{Html.RenderAction("MenuLeft", "Admin");}
        </aside>
        <div class="content-wrapper">
            @RenderBody()
        </div>
        <footer class="main-footer">
            @{Html.RenderAction("Footer", "Admin");}
        </footer>
    </div> 
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/plugins/pfrom")
    @Scripts.Render("~/bundles/modernizr")
        @Scripts.Render("~/bundles/jqueryval")
    <script src="~/Theme/plugins/iCheck/icheck.min.js"></script>
    @Scripts.Render("~/bundles/dist")
    @RenderSection("scripts", required: false)
</body>
</html>
